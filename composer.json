{
    "name": "crayfi/cray-laravel",
    "description": "First-class Laravel Composer package for integrating Cray Finance APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cray Finance",
            "email": "dev@crayfi.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Cray\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cray\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cray\\Laravel\\CrayServiceProvider"
            ],
            "aliases": {
                "Cray": "Cray\\Laravel\\Facades\\Cray"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
